<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
  <display-name>SUP-WS-EJB</display-name>

  	<enterprise-beans>
		<session>
			<ejb-name>OrderBean</ejb-name>
			<service-endpoint>com.tobias.sup.ejb.OrderInterface</service-endpoint>
			<ejb-class>com.tobias.sup.ejb.OrderBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>ProductBean</ejb-name>
			<service-endpoint>com.tobias.sup.ejb.ProductInterface</service-endpoint>
			<ejb-class>com.tobias.sup.ejb.ProductBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>UserBean</ejb-name>
			<service-endpoint>com.tobias.sup.ejb.UserInterface</service-endpoint>
			<ejb-class>com.tobias.sup.ejb.UserBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>

</ejb-jar>